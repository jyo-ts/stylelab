<div class="container main-page">
  <div class="users-title">
    <h1>Stylist 一覧</h1>
  </div>
    <hr>
    <div class="user-icon">
      <% @users.each do |u| %>
        <% if User.find(u.id).user_type == "s" %>
          <div class="well">
            <h3><a href="/users/<%= u.id %>"><%= image_tag User.find(u.id).image.url(:thumb) %> <%= User.find(u.id).first_name %> <%= User.find(u.id).last_name %>様</a></h3>
          </div>
        <% end %>
      <% end %>
    </div>
</div>